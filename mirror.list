#### variable ####
set base_path /media/redhawk/REPOS
set nthreads 43

#### config ####
set var_path $base_path/.var
set skel_path $var_path/skel
set postmirror_script $base_path/.postmirror.sh
set _autoclean 1
set run_postmirror 0

